    "@types/styled-components": "^4.4.3",
    "@types/systemjs": "^0.20.7",
    "@types/webpack": "^4.41.6",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "add-asset-html-webpack-plugin": "3.1.3",
